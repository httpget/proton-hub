<body class="dark-mode">
    <div id="sidebar">
        <div class="Sidebar-header">
            <div class="Sidebar-header-row">
                <div class="sidebar-title">ProtoSmasher Docs</div>
            </div>
        </div>
        <nav role="navigation" class="sidebar-nav" id="Sidebar-scroll"></nav>
    </div>
    <div id="content" tabindex="0"></div>
    <script>
        function addSection(SectionName, Methods, Classes) {
            // sidebar nav stuff
            var appendName = (SectionName == "Base") ? "" : SectionName + ".";
            if (SectionName == "Debug")
                appendName = "d" + appendName.substring(1);

            var addSideBarItem = () => {
                var group = document.createElement("div");
                group.setAttribute("class", "sidebar-nav-group");

                var heading = document.createElement("h5");
                heading.setAttribute("class", "sidebar-nav-heading");
                heading.innerHTML = SectionName;
                group.appendChild(heading);

                var items = document.createElement("ul");
                items.setAttribute("class", "sidebar-nav-items");

                Methods.forEach(element => {
                    var li = document.createElement("li");
                    var a = document.createElement("a");
                    a.setAttribute("class", "sidebar-nav-item");
                    a.setAttribute("href", "#" + SectionName + "-" + element.name);
                    a.setAttribute("id", "sidebar-" + SectionName + "-" + element.name);
                    a.innerHTML = element.name;
                    li.appendChild(a);
                    items.appendChild(li);
                });

                if (Classes != undefined) {
                    Classes.forEach(c => {
                        var li = document.createElement("li");
                        var a = document.createElement("a");
                        a.setAttribute("class", "sidebar-nav-item");
                        a.setAttribute("href", "#" + SectionName + "-" + c.name);
                        a.setAttribute("id", "sidebar-" + SectionName + "-" + c.name);
                        a.innerHTML = c.name + " (Class)";
                        li.appendChild(a);
                        items.appendChild(li);
                    });
                }

                group.appendChild(items);
                document.getElementById("Sidebar-scroll").appendChild(group);
            };

            var addContent = () => {

                Methods.forEach(element => {
                    var section = document.createElement("section");
                    section.setAttribute("class", "method first-of-group");
                    section.setAttribute("id", SectionName + "-" + element.name);

                    var divider = document.createElement("div");
                    divider.setAttribute("class", "MethodArea-divider");
                    section.appendChild(divider);

                    var methodArea = document.createElement("div");
                    methodArea.setAttribute("class", "method-area method-area-expanded");

                    var copy = document.createElement("div");
                    copy.setAttribute("class", "method-copy");

                    var padding = document.createElement("div");
                    padding.setAttribute("class", "method-copy-padding");

                    var title = document.createElement("div");
                    title.setAttribute("class", "MethodCopyTitle");
                    var h1 = document.createElement("h1");
                    h1.setAttribute("class", "MethodCopyTitle-anchor");
                    h1.innerHTML = appendName + element.name;
                    title.append(h1);

                    var codeExample = document.createElement("p");
                    var code = "<div class=\"codeStyle\">";
                    code += "<span class=\"code-return\">" + element.return.replace(/\</g, "&lt;").replace(
                        /\>/g, "&gt;") + " </span>";
                    code += "<span class=\"code-name\">" + appendName + element.name.replace(/\</g, "&lt;")
                        .replace(
                            /\>/g, "&gt;") + "</span>(";
                    element.args.split(',').forEach(s => {
                        if (s == "<none>") {
                            code += "<span class=\"code-type\">&lt;none&gt;</span>  ";
                            return;
                        }

                        var defaultValue = s.match(/(\s=\s.+)/) || [""];
                        defaultValue = defaultValue[0];
                        s = s.replace(/(\s=\s.+)/, "");


                        var split = s.split(/\s(?=[^\s]+$)/);;
                        var cleaned = split[0].replace(/\</g, "&lt;")
                            .replace(/\>/g, "&gt;")
                            .replace(/\?/g, ",");

                        code += "<span class=\"code-type\">" + cleaned + "</span>";
                        code += " " + split[1] + "<span class=\"code-default\">" + defaultValue +
                            "</span>, ";
                    });
                    code = code.substring(0, code.length - 2);
                    code += ")</div>";
                    codeExample.innerHTML = code;


                    var desc = document.createElement("p");
                    desc.innerHTML = element.desc;

                    padding.appendChild(title);
                    padding.appendChild(codeExample);
                    padding.append(desc);
                    copy.appendChild(padding);

                    methodArea.append(copy);
                    section.append(methodArea);

                    if (Classes != undefined) {

                        methodArea.style = "padding-bottom: 2vw !important;";

                        Classes.forEach(c => {
                            var classElement = document.createElement("section");
                            classElement.setAttribute("class", "method");
                            classElement.setAttribute("id", SectionName + "-" + c.name);

                            var divider = document.createElement("div");
                            divider.setAttribute("class", "MethodArea-divider");
                            divider.style =
                                "margin-left: var(--method-area-padding); width: calc(100% - var(--method-area-padding) * 2); max-width: 100%;";
                            classElement.appendChild(divider);

                            var methodArea = document.createElement("div");
                            methodArea.setAttribute("class", "method-area method-area-expanded");
                            methodArea.style = "padding-top: 2vw;"

                            var copy = document.createElement("div");
                            copy.setAttribute("class", "method-copy");

                            var padding = document.createElement("div");
                            padding.setAttribute("class", "method-copy-padding");

                            var title = document.createElement("div");
                            title.setAttribute("class",
                                "MethodCopyTitle MethodCopyTitle-subsection");
                            var h1 = document.createElement("h1");
                            h1.setAttribute("class", "MethodCopyTitle-anchor");
                            h1.innerHTML = c.name + " (Class)";

                            var props = document.createElement("div");
                            props.setAttribute("class", "method-list");

                            var propsTitle = document.createElement("h5");
                            propsTitle.setAttribute("class", "method-list-title");
                            propsTitle.innerHTML = "Properties";

                            var propsGroup = document.createElement("ul");
                            propsGroup.setAttribute("class", "method-list-group");

                            c.props.forEach(p => {
                                var prop = document.createElement("li");
                                prop.setAttribute("class", "method-list-item");

                                var codeExample = document.createElement("p");
                                codeExample.style = "margin-top: 5px !important;";

                                var code = "<div class=\"codeStyle\">";
                                code += "<span class=\"code-return\">" + p.type
                                    .replace(/\</g, "&lt;").replace(
                                        /\>/g, "&gt;") + " </span>";
                                code += "<span class=\"code-name\">" + p
                                    .name.replace(/\</g, "&lt;")
                                    .replace(
                                        /\>/g, "&gt;") + "</span>";
                                if (p.args != undefined) {
                                    code += "(";
                                    p.args.split(',').forEach(s => {
                                        if (s == "<none>") {
                                            code +=
                                                "<span class=\"code-type\">&lt;none&gt;</span>  ";
                                            return;
                                        }

                                        var defaultValue = s.match(/(\s=\s.+)/) || [
                                            ""
                                        ];
                                        defaultValue = defaultValue[0];
                                        s = s.replace(/(\s=\s.+)/, "");

                                        var split = s.split(/\s(?=[^\s]+$)/);;
                                        var cleaned = split[0].replace(/\</g,
                                                "&lt;")
                                            .replace(/\>/g, "&gt;")
                                            .replace(/\?/g, ",");

                                        code += "<span class=\"code-type\">" +
                                            cleaned +
                                            "</span>";
                                        code += " " + split[1] +
                                            "<span class=\"code-default\">" +
                                            defaultValue +
                                            "</span>, ";
                                    });
                                    code = code.substring(0, code.length - 2);
                                    code += ")";
                                }
                                code += "<span class=\"code-default\">" + (p.attr ? " " + p
                                    .attr : "") + "</span></div>";
                                codeExample.innerHTML = code;

                                prop.appendChild(codeExample);

                                propsGroup.appendChild(prop);
                            });

                            var methods = document.createElement("div");
                            methods.setAttribute("class", "method-list");

                            var methodsTitle = document.createElement("h5");
                            methodsTitle.setAttribute("class", "method-list-title");
                            methodsTitle.innerHTML = "Methods";

                            var methodsGroup = document.createElement("ul");
                            methodsGroup.setAttribute("class", "method-list-group");

                            c.methods.forEach(p => {
                                var method = document.createElement("li");
                                method.setAttribute("class", "method-list-item");

                                var codeExample = document.createElement("p");
                                codeExample.style = "margin-top: 5px !important;";

                                var code = "<div class=\"codeStyle\">";
                                code += "<span class=\"code-return\">" + p.return
                                    .replace(/\</g, "&lt;").replace(
                                        /\>/g, "&gt;") + " </span>";
                                code += "<span class=\"code-name\">" + p
                                    .name.replace(/\</g, "&lt;")
                                    .replace(
                                        /\>/g, "&gt;") + "</span>(";
                                if (p.args != undefined) {
                                    p.args.split(',').forEach(s => {
                                        if (s == "<none>") {
                                            code +=
                                                "<span class=\"code-type\">&lt;none&gt;</span>  ";
                                            return;
                                        }

                                        var defaultValue = s.match(/(\s=\s.+)/) || [
                                            ""
                                        ];
                                        defaultValue = defaultValue[0];
                                        s = s.replace(/(\s=\s.+)/, "");

                                        var split = s.split(/\s(?=[^\s]+$)/);;
                                        var cleaned = split[0].replace(/\</g,
                                                "&lt;")
                                            .replace(/\>/g, "&gt;")
                                            .replace(/\?/g, ",");

                                        code += "<span class=\"code-type\">" +
                                            cleaned +
                                            "</span>";
                                        code += " " + split[1] +
                                            "<span class=\"code-default\">" +
                                            defaultValue +
                                            "</span>, ";
                                    });
                                }
                                code = code.substring(0, code.length - 2);
                                code += ")</div>";
                                codeExample.innerHTML = code + p.desc + "\n";

                                method.appendChild(codeExample);

                                methodsGroup.appendChild(method);
                            });

                            props.appendChild(propsTitle);
                            props.appendChild(propsGroup);

                            methods.appendChild(methodsTitle);
                            methods.appendChild(methodsGroup);

                            title.appendChild(h1);
                            padding.appendChild(title);

                            copy.appendChild(padding);
                            if (c.props.length != 0)
                                copy.appendChild(props);
                            if (c.methods.length != 0)
                                copy.appendChild(methods);

                            methodArea.appendChild(copy);
                            classElement.appendChild(methodArea);
                            section.appendChild(classElement);
                        });

                    }

                    document.getElementById("content").appendChild(section);
                });
            };

            addSideBarItem();
            addContent();
        }

        var handleSelect = () => {
            document.querySelectorAll(".selected").forEach(e => {
                e.setAttribute("class", e.getAttribute("class").replace("selected", "").trim())
            });

            var body = document.body,
                html = document.documentElement;

            var height = Math.max(body.scrollHeight, body.offsetHeight, html.clientHeight, html
                .scrollHeight, html.offsetHeight);
            var scrollElement = document.getElementById("Sidebar-scroll");

            var elementId = document.elementFromPoint(281, 10).parentElement.id;
            var element = document.getElementById("sidebar-" + elementId);

            element.setAttribute("class", element.getAttribute("class").trim() + " selected");
            element.scrollIntoView(false);

            if (scrollElement.scrollTop != 0)
                scrollElement.scrollTop += 100;
        };
        document.getElementById("content").onscroll = handleSelect;

        addSection("Base", [{
                name: "getgenv",
                return: "table",
                args: "<none>",
                desc: "Returns the global environment table for ProtoSmasher."
            },
            {
                name: "getrenv",
                return: "table",
                args: "<none>",
                desc: "Returns the table of all roblox globals."
            },
            {
                name: "getrawmetatable",
